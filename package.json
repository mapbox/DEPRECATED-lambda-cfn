{
  "name": "@mapbox/lambda-cfn",
  "version": "0.1.5",
  "description": "CloudFormation boiler plate for lambda functions",
  "main": "lib/lambda-cfn.js",
  "engines": {
    "node": "0.10.36 || 4.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mapbox/lambda-cfn.git"
  },
  "scripts": {
    "test": "NODE_ENV=test tape test/*.test.js test/**/*.test.js"
  },
  "bin": {
    "lambda-cfn-template": "./bin/lambda-cfn-template.js"
  },
  "dependencies": {
    "streambot": "3.4.0",
    "d3-queue": "^2.0.3",
    "app-root-path": "1.2.0",
    "aws-sdk": "^2.2.35"
  },
  "devDependencies": {
    "jscs": "^1.11.3",
    "jshint": "^2.6.3",
    "tape": "^4.5.1",
    "nock": "^7.7.2"
  },
  "jscsConfig": {
    "preset": "airbnb",
    "requireCamelCaseOrUpperCaseIdentifiers": null
  },
  "jshintConfig": {
    "node": true,
    "globalstrict": false,
    "undef": true,
    "unused": true,
    "noarg": true
  },
  "author": "Mapbox, Inc.",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/mapbox/lambda-cfn/issues"
  },
  "homepage": "https://github.com/mapbox/lambda-cfn"
}